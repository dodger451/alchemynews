{
    "name": "dodger451/alchemynews",
    "description": "Test running Alechemynews API on heroku",
    "keywords": ["travis", "template"],
    "homepage": "http://david.latotzky.de",
    "version": "1.0",
    "authors": [
        {
            "name": "David Latotzky",
            "homepage": "http://david.latotzky.de/dev"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpdocumentor/phpdocumentor": "2.*",
        "silex/silex": "~1.3",
        "monolog/monolog": "~1.7",
        "twig/twig": "^1.19",
        "symfony/twig-bridge": "^2.7",
        "alrik11es/cowsayphp": "^1.0",
        "herrera-io/silex-pdo": "^1.2"
    },
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": "~4.5",
        "heroku/heroku-buildpack-php": "*"
    },
    "recommend": {
    },
    "autoload": {
        "psr-4": {
            "Latotzky\\Alchemynews\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Latotzky\\Alchemynews\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
